<html>
  <head>
    <title>KWIK-E-MART</title>
    <script src="https://unpkg.com/history@4.9.0"></script>
    <script src="/c/js/pages.js" type="module" async></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <link href="/c/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />
  </head>
  <body>
    <c-pages></c-pages>
  </body>
  <script>
      document.addEventListener("a:location:changed", (e) => {
        if (e.detail.href) {
          window.appHistory.push(e.detail.href);
        }
      });
      window.appHistory = window.History.createBrowserHistory();
  </script>
</html>
